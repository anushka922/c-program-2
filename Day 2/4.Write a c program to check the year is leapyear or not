#include<stdio.h>
int main()
{
int year;
printf("Enter any year:");
scanf("%d",&year);
if(year%400==0)
{
printf("%d is leapyear",year);
}
elseif(year%100 !=0)
{
printf("%d is not a leapyear",year);
}
elseif(year%4==0)
{
printf("%d is leapyear",year);
}
else
{
printf("%d is not a leapyear",year);
}
return 0;
}
